{
	"env": {
        "es6": true,
        "browser": true
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": false,
        "modules": true
    },
    "rules" : {
        "no-unused-vars" : 2,
        "no-undef" : 2
    },
	"parserOptions": {		
		"sourceType": "module",
